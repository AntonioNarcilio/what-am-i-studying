{
  "compilerOptions":{
     "target":"es6",
     "lib":[
        "dom",
        "dom.iterable",
        "esnext"
     ],
     "allowJs":true,
     "skipLibCheck":true,
     "esModuleInterop":true,
     "allowSyntheticDefaultImports":true,
     "strict":true,
     "forceConsistentCasingInFileNames":true,
     "noFallthroughCasesInSwitch":true,
     "module":"esnext",
     "moduleResolution":"node",
     "resolveJsonModule":true,
     "isolatedModules":true,
     "noEmit":true,
     "jsx":"preserve",
     "baseUrl":"./src",
     "paths":{
        "*":[
           "node_modules/*"
        ],
        "@/contexts/*":[
          "./contexts/*"
        ],
        "@/components/*":[
          "./components/*"
        ],
        "@/pages/*":[
          "./pages/*"
        ],
        "@/styles/*":[
          "./styles/*"
        ],
        "@/themes/*":[
          "./themes/*"
        ],
        "@/types/*":[
          "./@types/*"
        ],
        "@/utils/*":[
          "./utils/*"
        ]
     }
  },
  "include":[
     "src",
     "**/*.ts",
     "**/*.tsx"
  ],
  "exclude":[
     "node_modules"
  ],
  "files":[
     "src/@types/styled.d.ts"
  ]
}
